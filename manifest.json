{
    "manifest_version": 2,
    "name": "Auto index",
    "version": "0.0.1",
  
    "description": "A plain text description",
  
    "browser_action": {
        "default_title": "Google Index",
        "default_popup": "popup.html"
      },
    "content_scripts": [
      { 
        "matches": ["http://*/*", "https://*/*"],
        "js": ["gen-index.js"]
      }
    ],
  
    "author": "Ngô Ngọc Đức Huy",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions": ["activeTab"],
    "version_name": "0.0.1"
  }